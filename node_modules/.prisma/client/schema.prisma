datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Feed {
  id    String @id @default(uuid())
  title String
  link  String @unique
}

model User {
  id           String @id @default(uuid())
  username     String @unique
  password     String
  removedFeeds Json   @default("[]")
}
